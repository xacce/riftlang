<?xml version="1.0" encoding="UTF-8"?>
<NotepadPlus>
    <UserLang name="RiftLang" ext="rift" udlVersion="2.1">
        <Settings>
            <Global caseIgnored="no" allowFoldOfComments="no" />
            <Prefix Keywords1="no" Keywords2="no" Keywords3="no" Keywords4="no" />
        </Settings>

        <KeywordLists>
            <Keywords name="Keywords1">input settings print if elif stop update fetch from</Keywords>
            <Keywords name="Keywords2">and or</Keywords>
            <Keywords name="Keywords3">int bool float</Keywords>
            <Keywords name="Keywords4"></Keywords>
            <Keywords name="Keywords5"></Keywords>
            <Keywords name="Keywords6"></Keywords>
            <Keywords name="Keywords7"></Keywords>
            <Keywords name="Keywords8"></Keywords>

            <Keywords name="Operators1">= == .</Keywords>
            <Keywords name="Operators2"></Keywords>

            <Keywords name="Delimiters1"></Keywords>
            <Keywords name="Delimiters2"></Keywords>
            <Keywords name="Delimiters3"></Keywords>
            <Keywords name="Delimiters4"></Keywords>
            <Keywords name="Delimiters5"></Keywords>
            <Keywords name="Delimiters6"></Keywords>
            <Keywords name="Delimiters7"></Keywords>
            <Keywords name="Delimiters8"></Keywords>
        </KeywordLists>

        <Styles>
            <WordsStyle name="DEFAULT" fgColor="000000" bgColor="FFFFFF" />
            <WordsStyle name="KEYWORD1" fgColor="0000FF" fontStyle="1" />
            <WordsStyle name="KEYWORD2" fgColor="800080" />
            <WordsStyle name="KEYWORD3" fgColor="0080FF" />
            <WordsStyle name="OPERATOR" fgColor="800000" />
        </Styles>
    </UserLang>
</NotepadPlus>
